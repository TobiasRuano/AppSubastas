<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/titleTextView5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SubastaActivity"
    >


    <RelativeLayout

        android:id="@+id/relativeLayout5"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:id="@+id/primerLinearLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/historialPujasView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16sp"
                android:layout_marginLeft="16sp"
                android:text="⏱️"
                android:textColor="@android:color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                android:layout_marginVertical="5dp"
                />

            <TextView
                android:id="@+id/titleDescriptionTextView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Description"
                android:textColor="@android:color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                android:fontFamily="@font/montserrat_medium"
                android:layout_marginVertical="5dp"
                />


        </LinearLayout>



        <LinearLayout
            android:id="@+id/segundoLinearLayout5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_below="@+id/primerLinearLayout5">

            <org.imaginativeworld.whynotimagecarousel.ImageCarousel
                android:id="@+id/carousel5"
                android:layout_width="match_parent"
                android:layout_height="300dp" />

            <TextView
                android:id="@+id/estadoDetalleDescriptionTextView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Ganada"
                android:gravity="center"
                android:textColor="#00FF00"
                android:textSize="25sp"
                android:textStyle="bold"
                android:fontFamily="@font/montserrat_medium"
                android:layout_marginRight="16dp"
                android:layout_marginTop="10dp"
                />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="0dp">


        <TextView
            android:id="@+id/valorActualDescriptionTextView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/titleDescriptionTextView5"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:text="Valor (info)"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="30sp"
            android:fontFamily="@font/montserrat_medium"
            />

            <TextView
                android:id="@+id/monedaActualDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text="ARS"
                android:textColor="@android:color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                android:fontFamily="@font/montserrat_medium"
               />


            <TextView
                android:id="@+id/valorActualOVendido5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="16dp"
                android:text="Valor Actual"
                android:textColor="#009fff"
                android:textStyle="bold"
                android:textSize="18sp"
                android:fontFamily="@font/montserrat_medium_italic"
                />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        <TextView
            android:id="@+id/precioBaseDescriptionTextView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/precioBaseDescriptionTextView5"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="10dp"
            android:text="Precio Base (info)"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            android:textSize="16sp"

            android:fontFamily="@font/montserrat_medium_italic"
            />

            <TextView
                android:id="@+id/monedaBaseDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/precioBaseDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:text="ARS"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                android:textStyle="bold"
                android:fontFamily="@font/montserrat_medium_italic"/>


            <TextView
                android:id="@+id/precioBaseView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/precioBaseDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="10dp"
                android:text="Precio Base"
                android:textColor="@android:color/black"
                android:textStyle="bold"
                android:textSize="16sp"
                android:fontFamily="@font/montserrat_medium_italic"/>

        </LinearLayout>


            <View
                android:id="@+id/divider"
                android:layout_width="match_parent"
                android:layout_marginTop="8dp"
                android:layout_height="2dp"
                android:background="?android:attr/listDivider" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:text="Descripción"
                android:textColor="@android:color/black"
                android:textSize="25sp"
            android:fontFamily="@font/montserrat_regular"/>

            <TextView
                android:id="@+id/fullTitleDescriptionTextView5"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:text="Descripcion Completa (info)"
                android:textColor="@android:color/black"
                android:textSize="20dp"
                android:fontFamily="@font/montserrat_regular"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/full2TitleDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:text="Ver descripción completa "
                android:fontFamily="@font/montserrat_medium_italic"
                android:textColor="#3483fa"
                />



            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:srcCompat="@drawable/ic_downarrow_121316"
                android:gravity="bottom"
                android:fontFamily="@font/montserrat_medium"
                android:textStyle="bold"
                android:layout_marginTop="2dp"

                />


        </LinearLayout>


            <View
                android:id="@+id/divider2"
                android:layout_width="match_parent"
                android:layout_marginTop="8dp"
                android:layout_height="2dp"
                android:background="?android:attr/listDivider" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="0dp"
                android:text="Estado del Envio"
                android:textColor="@android:color/black"
                android:textSize="25sp"
                android:fontFamily="@font/montserrat_regular"/>


            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="bottom"
            android:layout_marginBottom="5dp"
            >

            <TextView
                android:id="@+id/pendienteEnvioDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:text="Pendiente ->"
                android:textColor="@android:color/black"
                android:textSize="20sp"
                android:fontFamily="@font/montserrat_regular"/>

            <TextView
                android:id="@+id/enviadoEnvioDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:text="Enviado ->"
                android:textColor="@android:color/black"
                android:textSize="20sp"
                android:fontFamily="@font/montserrat_regular"/>

            <TextView
                android:id="@+id/recibidoEnvioDescriptionTextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/titleDescriptionTextView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:text="Recibido"
                android:textColor="@android:color/black"
                android:textSize="20sp"
                android:fontFamily="@font/montserrat_regular"/>


        </LinearLayout>



        </LinearLayout>


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>